{
  "name": "kptlex/yii2-container",
  "description": "PSR-11 dependency injection container for Yii2",
  "keywords": [
    "dependency",
    "injection",
    "container",
    "psr-11",
    "di",
    "yii2"
  ],
  "type": "library",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/kptlex/yii2-container/issues?state=open",
    "source": "https://github.com/kptlex/yii2-container"
  },
  "authors": [
    {
      "name": "Alexandr Satyshev",
      "email": "satyshev@websooby.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Lex\\Yii2\\Container\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lex\\Yii2\\Container\\Tests\\": "tests"
    }
  },
  "require": {
    "php": "^7.4|^8.0",
    "psr/container": "^1.0",
    "yiisoft/yii2": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.4"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ]
}
